<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mobileappapplication" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".startActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".startActivity$EditorPreferenceActivity"
            android:label="Game Setting" />
        <activity
            android:name=".scoresActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".easyActivity"
            android:label="Act and React" />
        <activity
            android:name=".mediumActivity"
            android:label="Act and React" />
        <activity
            android:name=".hardActivity"
            android:label="Act and React" />

    </application>

</manifest>
